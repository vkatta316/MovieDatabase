extends layout

block content
  header(id="list-header")
    h1= title
  #movies-wrapper
    .movie
      img.img-rounded.logo(src=`${movie.ImageURL}`)
    .movie-info
      a(class = "movie-info" href=`/movies/${movie.id}`)= movie.title
      | 
      h3= movie.year
      | 
      h3= movie.synopsis
     
